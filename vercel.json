{
    "buildCommand": "pip install -r requirements.txt",
    "framework": "django",
    "functions": {
        "api/wsgi.py": {
            "maxDuration": 60
        }
    },
    "outputDirectory": ".",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/wsgi.py"
        }
    ]
}